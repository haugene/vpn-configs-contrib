client
dev tun
resolv-retry infinite
nobind
persist-key
persist-tun
verb 3
remote-cert-tls server
ping 10
ping-restart 60
sndbuf 524288
rcvbuf 524288
cipher AES-256-CBC
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384:TLS-DHE-RSA-WITH-AES-256-CBC-SHA
proto udp
auth-user-pass mullvad_userpass.txt
ca mullvad_ca.crt
tun-ipv6
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
fast-io
remote-random
remote fr-par-001.relays.mullvad.net 1196
remote fr-par-003.relays.mullvad.net 1196
remote fr-par-002.relays.mullvad.net 1196
remote fr-par-201.relays.mullvad.net 1196
remote fr-par-203.relays.mullvad.net 1196
remote fr-par-202.relays.mullvad.net 1196
remote fr-par-004.relays.mullvad.net 1196