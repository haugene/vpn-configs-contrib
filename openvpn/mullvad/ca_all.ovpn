client
dev tun
resolv-retry infinite
nobind
persist-key
persist-tun
verb 3
remote-cert-tls server
ping 10
ping-restart 60
sndbuf 524288
rcvbuf 524288
cipher AES-256-CBC
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384:TLS-DHE-RSA-WITH-AES-256-CBC-SHA
proto udp
auth-user-pass /config/openvpn-credentials.txt
ca /etc/openvpn/mullvad/ca.crt
script-security 2
#up /etc/openvpn/update-resolv-conf
#down /etc/openvpn/update-resolv-conf
fast-io
remote-random
remote ca-mtr-101.relays.mullvad.net 1194
remote ca-mtr-102.relays.mullvad.net 1194
remote ca-mtr-103.relays.mullvad.net 1194
remote ca-mtr-104.relays.mullvad.net 1194
remote ca-mtr-105.relays.mullvad.net 1194
remote ca-mtr-106.relays.mullvad.net 1194
remote ca-mtr-107.relays.mullvad.net 1194
remote ca-mtr-108.relays.mullvad.net 1194
remote ca-tor-101.relays.mullvad.net 1194
remote ca-tor-102.relays.mullvad.net 1194
remote ca-tor-103.relays.mullvad.net 1194
remote ca-tor-ovpn-001.relays.mullvad.net 1194
remote ca-tor-ovpn-002.relays.mullvad.net 1194
remote ca-van-003.relays.mullvad.net 1194
remote ca-van-004.relays.mullvad.net 1194