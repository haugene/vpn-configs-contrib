client
dev tun
resolv-retry infinite
nobind
persist-key
persist-tun
verb 3
remote-cert-tls server
ping 10
ping-restart 60
sndbuf 524288
rcvbuf 524288
cipher AES-256-CBC
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384:TLS-DHE-RSA-WITH-AES-256-CBC-SHA
proto udp
auth-user-pass mullvad_userpass.txt
ca mullvad_ca.crt
tun-ipv6
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
fast-io
remote-random
remote no-osl-ovpn-002.relays.mullvad.net 1195
remote no-osl-ovpn-007.relays.mullvad.net 1195
remote no-osl-ovpn-001.relays.mullvad.net 1195
remote no-osl-ovpn-003.relays.mullvad.net 1195
remote no-osl-ovpn-006.relays.mullvad.net 1195
remote no-osl-ovpn-004.relays.mullvad.net 1195
remote no-osl-ovpn-005.relays.mullvad.net 1195
remote no-osl-ovpn-008.relays.mullvad.net 1195